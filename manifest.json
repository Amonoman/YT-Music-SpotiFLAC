{
  "name": "ytmusic-spotiflac",
  "displayName": "YouTube Music",
  "version": "1.4.0",
  "description": "YouTube Music Search Provider for SpotiFLAC. Search tracks, albums, and playlists on YouTube Music. Uses Odesli for ISRC lookup.",
  "author": "Amonoman",
  "type": ["metadata_provider"],
  "icon": "icon.jpg",
  "minAppVersion": "3.0.1",
  "permissions": {
    "network": ["music.youtube.com", "*.youtube.com", "www.youtube.com", "i.ytimg.com", "api.song.link", "odesli.io"],
    "storage": true,
    "file": false
  },
  "searchBehavior": {
    "enabled": true,
    "placeholder": "Search YouTube Music...",
    "thumbnailRatio": "square"
  },
  "urlHandler": {
    "enabled": true,
    "patterns": ["music.youtube.com", "youtu.be", "www.youtube.com/watch", "music.youtube.com/playlist", "music.youtube.com/browse"]
  },
  "skipMetadataEnrichment": false,
  "skipBuiltInFallback": false
}

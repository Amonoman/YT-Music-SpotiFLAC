{
  "name": "ytmusic-spotiflac",
  "displayName": "YouTube Music",
  "version": "1.7.0",
  "description": "YouTube Music Search Provider for SpotiFLAC. Search tracks, albums, and playlists on YouTube Music. Uses Odesli for ISRC lookup. Supports search filters.",
  "author": "Amonoman and Zarz",
  "type": ["metadata_provider"],
  "icon": "icon.jpg",
  "minAppVersion": "3.2.0",
  "permissions": {
    "network": ["music.youtube.com", "*.youtube.com", "www.youtube.com", "i.ytimg.com", "api.song.link", "odesli.io", "api.deezer.com"],
    "storage": true,
    "file": false
  },
  "capabilities": {
    "homeFeed": true
  },
  "searchBehavior": {
    "enabled": true,
    "placeholder": "Search YouTube Music...",
    "thumbnailRatio": "square",
    "filters": [
      { "id": "tracks", "label": "Songs", "icon": "music" },
      { "id": "albums", "label": "Albums", "icon": "album" },
      { "id": "artists", "label": "Artists", "icon": "artist" },
      { "id": "playlists", "label": "Playlists", "icon": "playlist" }
    ]
  },
  "urlHandler": {
    "enabled": true,
    "patterns": ["music.youtube.com", "youtu.be", "www.youtube.com/watch", "music.youtube.com/playlist", "music.youtube.com/browse"]
  },
  "skipMetadataEnrichment": false,
  "skipBuiltInFallback": false
}
